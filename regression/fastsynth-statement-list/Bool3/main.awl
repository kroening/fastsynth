FUNCTION_BLOCK "Main"
VERSION : 0.1
   VAR_TEMP 
      NONDET0 : Bool;
      NONDET1 : Bool;
      NONDET2 : Bool;
      NONDET3 : Bool;
      expectedTrue : Bool;
      return : Bool;
   END_VAR

BEGIN
